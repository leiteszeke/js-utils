{
  "name": "@leiteszeke/utils",
  "version": "1.5.3",
  "description": "Library with utils function to use across different JS and TS projects.",
  "main": "build/index.js",
  "scripts": {
    "prepublish": "yarn build",
    "build:prod": "webpack --config webpack.prod.js",
    "build:dev": "webpack -w --config webpack.dev.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "prettier": "prettier --check ./src/**/*.ts",
    "prettier:fix": "prettier --write ./src/**/*.ts",
    "test": "jest --watch",
    "deploy": "yarn build && yarn publish"
  },
  "contributors": [
    {
      "name": "Ezequiel Leites",
      "email": "ezequiel@leites.dev"
    },
    {
      "name": "Nicolas Villalonga",
      "email": "nicovillalonga90@gmail.com"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/node-fetch": "^2.5.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-a11y": "^6.4.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-watch": "^7.0.0",
    "history": "^5.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "ts-loader": "^8.0.13",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
