{
  "name": "@leiteszeke/utils",
  "version": "1.2.1",
  "description": "Library with utils function to use across different JS and TS projects.",
  "main": "build/index.js",
  "scripts": {
    "build:prod": "webpack -p --config webpack.prod.js",
    "build:dev": "webpack -w --config webpack.dev.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix",
    "prettier": "prettier --check ./src/**/*.{js,jsx,ts,tsx}",
    "prettier:fix": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
    "test": "jest --watch"
  },
  "contributors": [
    {
      "name": "Ezequiel Leites",
      "email": "ezequiel@leites.dev"
    },
    {
      "name": "Nicolas Villalonga",
      "email": "nicovillalonga90@gmail.com"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@types/node-fetch": "^2.5.7",
    "@types/react-router-dom": "^5.1.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-watch": "^7.0.0",
    "history": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hooks-testing-library": "^0.6.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
